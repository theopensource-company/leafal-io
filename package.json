{
  "name": "leafal-io",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev-migrate": "ts-node ./scripts/migratedefaults.ts",
    "dev": "concurrently --names \"leafal,surrl\" \"pnpm run dev-run-next\" \"pnpm run dev-run-surreal\"",
    "dev-run-next": "next dev --port 14000",
    "dev-run-surreal": "surreal start --log debug --user root --pass root --bind 0.0.0.0:14001 file:dev.db",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@theopensource-company/surrealdb-cloudflare": "^1.0.4",
    "@types/express": "^4.17.17",
    "@types/node": "20.1.7",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "express": "^4.18.2",
    "next": "13.4.2",
    "node-fetch": "^3.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.62.1",
    "surrealdb.js": "^0.7.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  }
}
